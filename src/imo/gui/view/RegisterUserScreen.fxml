<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox prefHeight="243.0" prefWidth="487.0" spacing="5.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="imo.gui.controls.RegisterUserController">
   <children>
      <Label text="Full Name" />
      <TextField fx:id="fullNameTextField" alignment="CENTER" onKeyReleased="#updateFirstLastName" prefHeight="25.0" prefWidth="100.0" promptText="Full Name" />
      <Label text="CPF" />
      <TextField fx:id="cpfTextField" alignment="CENTER" prefHeight="25.0" prefWidth="100.0" promptText="00000000000" />
      <Label text="Password" />
      <PasswordField id="passInput" fx:id="passwordSigInTextField" alignment="CENTER" prefHeight="25.0" prefWidth="100.0" promptText="password" />
      <HBox alignment="CENTER" prefHeight="19.0" prefWidth="464.0" spacing="5.0">
         <children>
            <Label text="First Name" />
            <TextField fx:id="firstNameTextField">
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </TextField>
            <Label text="Last Name" />
            <TextField fx:id="lastNameTextField" />
         </children>
      </HBox>
      <Label text="Type" />
      <HBox alignment="TOP_CENTER" prefHeight="24.0" prefWidth="471.0" spacing="146.0">
         <children>
            <ComboBox fx:id="typeComboBox" prefHeight="25.0" prefWidth="250.0">
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </ComboBox>
            <Button alignment="CENTER_RIGHT" mnemonicParsing="false" onAction="#register" text="Register" />
         </children>
         <VBox.margin>
            <Insets bottom="1.0" left="1.0" right="1.0" top="1.0" />
         </VBox.margin>
      </HBox>
   </children>
   <padding>
      <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
   </padding>
</VBox>
